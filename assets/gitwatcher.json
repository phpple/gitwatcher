{
  "git_version": {
    "version": "2.2.0"
  },
  "standard": {
    "phpcs": "vendor/bin/phpcs",
    "target": "app/,src/",
    "mode": "update",
    "options": {
      "standard": "{$project.root}assets/rules/phpdefault.xml",
      "colors": true,
      "s": true
    }
  },
  "committer": {
    "mail_extension": ""
  },
  "composer": {},
  "branch": {
    "rebase": {
      "compare": "master",
      "excludes": "dev,test,demo,master"
    }
  }
}